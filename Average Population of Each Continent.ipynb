   "source": [
    "SELECT c1.continent, FLOOR(AVG(c2.population))\n",
    "FROM country AS c1\n",
    "INNER JOIN city AS c2\n",
    "ON c1.code = c2.countrycode\n",
    "GROUP BY c1.continent;"
